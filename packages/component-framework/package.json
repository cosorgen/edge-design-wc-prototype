{
  "name": "@mai-ui/component-framework",
  "description": "MAI Web Components Framework",
  "version": "0.0.1",
  "author": "Microsoft",
  "license": "UNLICENSED",
  "type": "module",
  "private": true,
  "exports": {
    ".": {
      "types": "./dist/dts/index.d.ts",
      "default": "./dist/esm/index.js"
    },
    "./*.js": {
      "types": "./dist/dts/*.d.ts",
      "default": "./dist/esm/*.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "npm run clean && npm run build:tsc",
    "build:tsc": "tsc -p tsconfig.build.json",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@mai-ui/utilities": "^0.0.1"
  }
}
