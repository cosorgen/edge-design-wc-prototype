{
  "name": "@mai-ui/component-framework",
  "description": "MAI Web Components Framework",
  "version": "0.0.1",
  "author": "Microsoft",
  "license": "UNLICENSED",
  "type": "module",
  "private": true,
  "exports": {
    ".": {
      "types": "./dist/dts/index.d.ts",
      "default": "./dist/esm/index.js"
    },
    "./design-system.js": {
      "types": "./dist/dts/design-system.d.ts",
      "default": "./dist/esm/design-system.js"
    },
    "./get-initials.js": {
      "types": "./dist/dts/get-initials.d.ts",
      "default": "./dist/esm/get-initials.js"
    },
    "./states.js": {
      "types": "./dist/dts/states.d.ts",
      "default": "./dist/esm/states.js"
    },
    "./toggle-state.js": {
      "types": "./dist/dts/toggle-state.d.ts",
      "default": "./dist/esm/toggle-state.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "npm run clean && npm run build:tsc",
    "build:tsc": "tsc -p tsconfig.build.json",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@mai-ui/utilities": "^0.0.1",
    "@mai-ui/design-tokens": "^0.0.1"
  }
}
