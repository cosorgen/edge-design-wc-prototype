{
    "name": "@mai-ui/composer",
    "version": "0.0.1",
    "author": "Microsoft",
    "private": true,
    "description": "MAI Design Copilot Composer custom element",
    "license": "UNLICENSED",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/dts/index.d.ts",
            "default": "./dist/esm/index.js"
        },
        "./definition.js": {
            "types": "./dist/dts/composer.definition.d.ts",
            "default": "./dist/esm/composer.definition.js"
        },
        "./template.js": {
            "types": "./dist/dts/composer.template.d.ts",
            "default": "./dist/esm/composer.template.js"
        },
        "./styles.js": {
            "types": "./dist/dts/composer.styles.d.ts",
            "default": "./dist/esm/composer.styles.js"
        },
        "./define.js": {
            "types": "./dist/dts/define.d.ts",
            "default": "./dist/esm/define.js"
        },
        "./package.json": "./package.json"
    },
    "sideEffects": [
        "./dist/esm/define.js"
    ],
    "files": [
        "*.md",
        "dist/*.js",
        "dist/*.d.ts"
    ],
    "dependencies": {
        "@mai-ui/component-framework": "^0.0.1",
        "@mai-ui/design-tokens": "^0.0.1"
    },
    "peerDependencies": {
        "@microsoft/fast-element": "^2.0.0"
    },
    "scripts": {
        "build": "npm run build:tsc",
        "build:tsc": "tsc -p tsconfig.build.json",
        "clean": "rimraf dist"
    }
}
