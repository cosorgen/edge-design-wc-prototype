{
  "name": "@mai-ui/design-tokens",
  "private": true,
  "version": "0.0.1",
  "author": "Microsoft",
  "description": "MAI Design System Tokens",
  "license": "UNLICENSED",
  "type": "module",
  "exports": {
    "./*.js": {
      "types": "./dist/dts/*.d.ts",
      "default": "./dist/esm/*.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "npm run clean && npm run generate && npm run build:tsc",
    "build:tsc": "tsc -p tsconfig.build.json",
    "clean": "rimraf dist",
    "generate": "node ./scripts/generate.js"
  }
}
