{
  "name": "@edge-design/phoenix-theme",
  "version": "0.0.1",
  "author": "Microsoft",
  "description": "MAI Phoenix theme for Edge",
  "license": "UNLICENSED",
  "type": "module",
  "private": true,
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/dts/index.d.ts",
      "default": "./dist/esm/index.js"
    },
    "./*.js": {
      "types": "./dist/dts/*.d.ts",
      "default": "./dist/esm/*.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "*.md",
    "dist/*.js",
    "dist/*.d.ts"
  ],
  "scripts": {
    "build": "npm run clean && npm run compile && node scripts/build.js && npm run test",
    "compile": "tsc",
    "clean": "rimraf dist",
    "test": "node scripts/test.js"
  },
  "devDependencies": {
    "@mai-ui/design-tokens": "^1.3.1",
    "esbuild": "^0.24.0"
  },
  "dependencies": {
    "@material/material-color-utilities": "^0.3.0",
    "@phoenixui/themes": "^0.4.12"
  }
}
