{
  "name": "@mai-ui/text-input",
  "version": "0.0.1",
  "author": "Microsoft",
  "description": "MAI Design text input custom element",
  "license": "UNLICENSED",
  "type": "module",
  "private": true,
  "exports": {
    ".": {
      "types": "./dist/dts/index.d.ts",
      "default": "./dist/esm/index.js"
    },
    "./definition.js": {
      "types": "./dist/dts/text-input.definition.d.ts",
      "default": "./dist/esm/text-input.definition.js"
    },
    "./template.js": {
      "types": "./dist/dts/text-input.template.d.ts",
      "default": "./dist/esm/text-input.template.js"
    },
    "./styles.js": {
      "types": "./dist/dts/text-input.styles.d.ts",
      "default": "./dist/esm/text-input.styles.js"
    },
    "./define.js": {
      "types": "./dist/dts/define.d.ts",
      "default": "./dist/esm/define.js"
    },
    "./options.js": {
      "types": "./dist/dts/text-input.options.d.ts",
      "default": "./dist/esm/text-input.options.js"
    },
    "./tokens.js": {
      "types": "./dist/dts/text-input.tokens.d.ts",
      "default": "./dist/esm/text-input.tokens.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": [
    "./dist/esm/define.js"
  ],
  "files": [
    "*.md",
    "dist/*.js",
    "dist/*.d.ts"
  ],
  "dependencies": {
    "@mai-ui/component-framework": "^0.0.1"
  },
  "peerDependencies": {
    "@fluentui/web-components": ">= 3.0.0-beta.63 || ^3.0.0",
    "@microsoft/fast-element": "^2.0.0"
  },
  "scripts": {
    "build": "npm run clean && npm run build:tsc",
    "build:tsc": "tsc -p tsconfig.build.json",
    "clean": "rimraf dist"
  }
}